<h2>Team details</h2>
<p><strong>Ttitle: </strong>{{teams.name}}</p>


{{#each teams.participants}}
<h3>Movie cast</h3>
<ul>
    <li><strong>Name:</strong>{{this.name}}</li>
    <li><strong>Occupation:</strong>{{this.occupation}}</li>
    <li><strong>Catch Phrase:</strong>{{this.catchPhrase}}</li>

</ul>
{{/each}}

<form method = "POST" action = "/movies/delete/{{movie._id}}">

<button type="submit">Delete Movie</button>

<a href = "/movies/edit/{{movie._id}}">Edit Movie</a>

</form>